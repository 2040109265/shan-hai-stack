{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib":["ESNext","DOM"],
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".", 
    "types": [],
    "paths": {
      "@/*": ["src/*"] 
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}